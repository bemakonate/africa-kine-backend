{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "name": "category"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true,
    "increments": true,
    "comment": ""
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "advanceSettings": {
      "type": "json",
      "route": "/plugins/restaurant-settings/categories",
      "useParam": true,
      "paramId": "id",
      "text": "Advance Settings"
    },
    "products": {
      "plugin": "restaurant-settings",
      "collection": "product",
      "via": "categories",
      "dominant": true
    },
    "subCategories": {
      "type": "component",
      "repeatable": true,
      "component": "testing.sub-category"
    }
  }
}